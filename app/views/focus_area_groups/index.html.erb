<p id="notice"><%= notice %></p>

<h1>Focus Area Groups</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @focus_area_groups.each do |focus_area_group| %>
      <tr>
        <td><%= link_to 'Show', focus_area_group %></td>
        <td><%= link_to 'Edit', edit_focus_area_group_path(focus_area_group) %></td>
        <td><%= link_to 'Destroy', focus_area_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Focus Area Group', new_focus_area_group_path %>
